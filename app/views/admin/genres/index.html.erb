<h2>ジャンル一覧・追加</h2>

<table>
  <thead>
    <tr>
      <th>ジャンル</th>
    </tr>
  </thead>
  <tbody>
    <% @genres.each do |genre| %>
    <tr>
      <td>
        <%= genre.name %>
      </td>

      <td><%= link_to "編集する", edit_admin_genre_path(genre) %></td>

    </tr>
    <% end %>
  </tbody>
</table>