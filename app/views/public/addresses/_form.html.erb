<%= form_with model:address do |f| %>
  <div class="form-group">
    <%= f.label :post_code, "郵便番号(ハイフンなし)" %>
    <%= f.text_field :post_code, autocomplete: "shipping postal-code" %>
  </div>
  <div class="form-group">
    <%= f.label :address, "住所" %>
    <%= f.text_field :address, autocomplete: "shipping address" %>
  </div>
  <div class="form-group">
    <%= f.label :name, "名前" %>
    <%= f.text_field :name, class: 'form-control address_name' %>
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>
