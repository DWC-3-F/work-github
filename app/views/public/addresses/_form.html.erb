<%= form_with model:address,local:true do |f| %>
  <div class="form-group">
    <%= f.label :"郵便番号" %>
    <%= f.text_field :postal_code, autocomplete: "shipping postal-code" %>
  </div>
  <div class="form-group">
    <%= f.label :"住所" %>
    <%= f.text_field :address, autocomplete: "shipping address" %>
  </div>
  <div class="form-group">
    <%= f.label :"名前" %>
    <%= f.text_field :name, class: 'form-control address_name' %>
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>
